# Database Configuration
POSTGRES_URL="postgresql://username:password@hostname:5432/dbname"
POSTGRES_PASSWORD="your_password_here"
POSTGRES_HOST="your_db_host"
POSTGRES_USER="your_db_user"
POSTGRES_DB="listings"
POSTGRES_PORT=5432

# AMPRE API Key
AMPERE_API_KEY="your_ampre_api_key_here"

# Google Maps API for geocoding
GOOGLE_MAPS_API_KEY="your_google_maps_api_key_here"
ENABLE_GEOCODING=true
ADDRESS_BATCH_SIZE=500
GEOCODE_DELAY_MS=200

# Replication Settings
REPLICATION_BATCH_SIZE=5000
REPLICATION_CONCURRENCY=50
SYNC_INTERVAL_MINUTES=15
RUN_ADDRESS_STANDARDIZATION=true

# API Settings
API_PORT=9696 