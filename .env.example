# Application Configuration
NODE_ENV=development
PORT=9630

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=listings
POSTGRES_USER=realtordb
POSTGRES_PASSWORD=strong_password_here

# AMPRE API Configuration
AMPERE_API_KEY=your_ampre_api_key_here

# API Rate Limits
REPLICATION_BATCH_SIZE=5000
REPLICATION_CONCURRENCY=50
API_TIMEOUT=60000
MEDIA_TIMEOUT=30000
SYNC_INTERVAL_MINUTES=30

# Address Standardization
RUN_ADDRESS_STANDARDIZATION=true
ENABLE_GEOCODING=true
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
ADDRESS_BATCH_SIZE=1000
GEOCODING_CONCURRENCY=10
GEOCODING_TIMEOUT=10000
GEOCODING_MAX_RETRIES=3
GEOCODING_RETRY_DELAY=2000
GEOCODE_CACHE_SIZE=10000

# Circuit Breaker Configuration
CIRCUIT_THRESHOLD=5
CIRCUIT_RESET_TIMEOUT=30000 